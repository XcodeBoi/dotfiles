
# wm mode per space
alt + shift - e : yabai -m space --layout float
alt + shift - w : yabai -m space --layout bsp

# window refocus controls
alt - w : yabai -m window --focus north
alt - d : yabai -m window --focus east
alt - s : yabai -m window --focus south
alt - a : yabai -m window --focus west

# application shortcuts
# \
cmd - 0x2A : open /Applications/kitty.app/

# space refocus controls
alt + shift - a : yabai -m space --focus prev
alt + shift - d : yabai -m space --focus next

